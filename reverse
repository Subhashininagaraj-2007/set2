import java.util.*;
class reverse
{
  public static void main(String[] args) {
      Scanner sc= new Scanner(System.in);
      int n= sc.nextInt();
      int ld;
      int sum =0;
      while (n>0)
      {
        ld=n%10;
        sum=10*sum+ld;
        n=n/10;
        
      }
      System.out.println(sum);
  }
}
